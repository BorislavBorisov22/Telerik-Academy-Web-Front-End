function GetBiggestNumber(args) {
    var firstNumber = +args[0];
    var secondNumber = +args[1];
    var thirdNumber = +args[2];

    var biggest = 0;
    if (firstNumber > secondNumber) {
        if (firstNumber > thirdNumber) {
            biggest = firstNumber;
        } else {
            biggest = thirdNumber;
        }
    } else {
        if (secondNumber > thirdNumber) {
            biggest = secondNumber;
        } else {
            biggest = thirdNumber;
        }
    }

    return biggest;
}